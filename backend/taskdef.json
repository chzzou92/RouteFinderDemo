{
    "family": "crow-backend-task",
    "containerDefinitions": [
        {
            "name": "crow-backend-container",
            "image": "118557105310.dkr.ecr.us-east-2.amazonaws.com/crow-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
                "environment": [
                {
                    "name": "GOOGLE_API_KEY",
                    "value": "AIzaSyB9Gv3QO1XNNa4zdQI3Ynj8pfwt6Qn_D-w"
                }
                ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/crow-backend",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
   
    "executionRoleArn": "arn:aws:iam::118557105310:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],

    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
